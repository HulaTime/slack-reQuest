{
  "name": "reQuest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tspath -f",
    "db:start": "docker-compose up -d db && pnpm run db:migration:up",
    "db:stop": "docker-compose down db --remove-orphans",
    "db:migration:create": "knex --knexfile ./database/knexfile.ts migrate:make",
    "db:migration:up": "knex --knexfile ./database/knexfile.ts migrate:latest",
    "db:migration:down": "knex --knexfile ./database/knexfile.ts migrate:rollback",
    "db:seed:create": "knex --knexfile ./database/knexfile.ts seed:make",
    "db:seed:up": "knex --knexfile ./database/knexfile.ts seed:run",
    "server:dev": "nodemon --watch './**/*.ts' --exec 'ts-node -r dotenv/config' -r tsconfig-paths/register src/api/server.ts",
    "server:start": "nodemon --watch './**/*.ts' --exec 'ts-node -r dotenv/config' -r tsconfig-paths/register src/api/server.ts",
    "test": "jest",
    "npm-update": "ncu -i --format group"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.8",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-sensible-style": "^0.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nock": "14.0.0-beta.19",
    "nodemon": "^3.1.7",
    "npm-check-updates": "^17.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tspath": "^2.6.8",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "env-var": "^7.5.0",
    "express": "^4.21.1",
    "knex": "^3.1.0",
    "pg": "^8.13.0",
    "pino": "^9.5.0",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "tsyringe": "^4.8.0",
    "zod": "^3.23.8"
  }
}
