{
  "name": "reQuest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tspath -f",
    "db:start": "docker-compose up -d db && pnpm run db:migration:up",
    "db:stop": "docker-compose down db --remove-orphans",
    "db:migration:create": "knex --knexfile ./src/db/knexfile.ts migrate:make",
    "db:migration:up": "knex --knexfile ./src/db/knexfile.ts migrate:latest",
    "db:migration:down": "knex --knexfile ./src/db/knexfile.ts migrate:rollback",
    "server:dev": "nodemon --watch './**/*.ts' --exec 'ts-node -r dotenv/config' -r tsconfig-paths/register src/api/server.ts",
    "server:start": "nodemon --watch './**/*.ts' --exec 'ts-node -r dotenv/config' -r tsconfig-paths/register src/api/server.ts",
    "test": "jest",
    "npm-update": "ncu -i --format group"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-sensible-style": "^0.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "npm-check-updates": "^16.14.20",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tspath": "^2.6.8",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "env-var": "^7.5.0",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "pino": "^9.1.0",
    "pino-http": "^10.1.0",
    "supertest": "^7.0.0"
  }
}
